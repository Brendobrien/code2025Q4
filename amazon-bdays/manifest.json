{
  "manifest_version": 3,
  "name": "Amazon Bdays",
  "version": "26.0",
  "permissions": ["tabs", "activeTab", "storage"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon.jpeg",
      "48": "images/icon.jpeg",
      "128": "images/icon.jpeg"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}
